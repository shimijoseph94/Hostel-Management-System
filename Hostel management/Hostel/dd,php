                    <script>
        $(document).foundation();

        $(document).ready(function() {
                        var slider = new homepageSlider();
            slider.init();
            
            if ($(".mediumsize").css("display") != "none") {
                $('#appdownloadModal').foundation('reveal', 'open');
            }

            if ($(".smallsize").css("display") != "none") {
                var heromargin =  +($(window).height() * 0.5 + 50);
                $('.topbackground').css('min-height', heromargin +'px');
            }  else {
                var heromargin =  +($(window).height() * 0.7 + 50);
                $('.topbackground').css('min-height', heromargin +'px');
            }

            $(window).on("debouncedresize", function() {
                if ($(".smallsize").css("display") != "none") {
                        var pickerwidth = $('.dp-checkout').width();
                        $('.picker__holder').css('max-width', pickerwidth + 18 + 'px');
                }
                else {
                    var heromargin =  +($(window).height() * 0.7 + 50);
                    $('.topbackground').css('min-height', heromargin +'px');
                }
            });
        });

        // SEARCH
        $('.autocomplete').one("change", function () {
            $('.homesearch').toggleClass('datesearch');
        });

        // set datepicker box width for small
        $( '.dp-checkin' ).click(function() {
            var pickerwidth = $('.dp-checkout').width();

            if ($(".smallsize").css("display") != "none") {
                $('.picker__holder').css('max-width', pickerwidth + 18 + 'px');
            }
        });
    </script>
        
